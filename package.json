{
  "name": "bussr",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "assemble-release": "cd android && ./gradlew clean && ./gradlew assembleRelease",
    "bundle-release": "cd android && ./gradlew clean && ./gradlew bundleRelease",
    "default-lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint": "tsc --noEmit && eslint ./src",
    "lint:fix": "tsc --noEmit && eslint --fix ./src",
